{
    "version": 2,
    "builds": [
      {
        "src": "public", 
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "public/index.html" 
      }
    ]
  }
  

  {
    "version": 2,
    "builds": [
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  